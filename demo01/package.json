{
  "name": "demo01",
  "version": "1.0.0",
  "description": "babel + webpack 开发的工具库",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "types": "types/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:es && npm run build:umd",
    "build:cjs": "rm -rf cjs && cross-env MODULE_TYPE=cjs babel src -d cjs",
    "build:es": "rm -rf es && cross-env MODULE_TYPE=es babel src -d es",
    "build:umd": "rm -rf umd && npm run build:umd-development && npm run build:umd-production",
    "build:umd-development": "cross-env MODULE_TYPE=umd webpack --mode=development",
    "build:umd-production": "cross-env MODULE_TYPE=umd webpack --mode=production",
    "test": "jest --coverage --verbose",
    "doc": "jsdoc src -d ./docs/"
  },
  "files": [
    "es",
    "cjs",
    "umd"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "minimist": "^1.2.5",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0"
  }
}
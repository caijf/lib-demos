{
  "name": "demo03",
  "version": "1.0.0",
  "description": "typescript + rollup 开发的工具库",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:es && npm run build:umd",
    "build:cjs": "rm -rf cjs && tsc --outDir cjs -d -t ES5 -m commonjs",
    "build:es": "rm -rf es && tsc --outDir es -d -t ES5 -m ES2015",
    "build:umd": "rm -rf umd && rollup -c rollup.config.mjs",
    "test": "jest --coverage --verbose",
    "doc": "rm -rf docs && typedoc --out docs src/index.ts"
  },
  "files": [
    "es",
    "cjs",
    "umd"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^29.5.1",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "rollup": "^3.21.0",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "tslib": "^2.5.0"
  }
}
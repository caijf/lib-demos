{
  "name": "demo03",
  "version": "1.0.0",
  "description": "typescript + rollup 开发的工具库",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:es && npm run build:umd",
    "build:cjs": "rm -rf cjs && tsc --outDir cjs -d -t ES5 -m commonjs",
    "build:es": "rm -rf es && tsc --outDir es -d -t ES5 -m ES2015",
    "build:umd": "rm -rf umd && npm run build:umd-development && npm run build:umd-production",
    "build:umd-development": "rollup -c",
    "build:umd-production": "cross-env NODE_ENV=production rollup -c",
    "test": "jest --coverage --verbose",
    "doc": "rm -rf docs && typedoc --out docs src/index.ts"
  },
  "files": [
    "es",
    "cjs",
    "umd"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.23",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "rollup": "^2.47.0",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  }
}
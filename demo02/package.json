{
  "name": "demo02",
  "version": "1.0.0",
  "description": "typescript + webpack 开发的工具库",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "types": "cjs/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:es && npm run build:umd",
    "build:cjs": "rm -rf cjs && tsc --outDir cjs",
    "build:es": "rm -rf es && tsc --outDir es",
    "build:umd": "rm -rf umd && npm run build:umd-development && npm run build:umd-production",
    "build:umd-development": "webpack --mode=development",
    "build:umd-production": "cross-env NODE_ENV=production webpack --mode=production",
    "test": "jest --coverage --verbose",
    "doc": "rm -rf docs && typedoc --out docs src/index.ts",
    "type:es": "tsc -m es2015 --outDir es",
    "type:cjs": "tsc -m commonjs --outDir cjs"
  },
  "files": [
    "es",
    "cjs",
    "umd"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "ts-loader": "^9.1.2",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  }
}